  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.autoComplete.addBrackets": true,
  "files.associations": {
    "*.org": "org",
    "**/templates/*.html": "django-html",
